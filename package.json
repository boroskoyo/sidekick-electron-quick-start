{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "concurrently --kill-others \"electron .\" \"npm run start-server\" \"vite\"",
    "dev-app": "concurrently --kill-others \"electron .\" \"npm run start-server\" \"npm run start-dev-client\"",
    "dev": "concurrently --kill-others \"npm run start-server\" \"npm run start-dev-client\"",
    "start-server": "node server.js",
    "start-dev-client": "vite",
    "build-client": "vite build"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "electron": "^20.1.0",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.2",
    "@runsidekick/sidekick-client": "^0.0.2",
    "axios": "^0.27.2",
    "concurrently": "^7.3.0",
    "electron-is-dev": "^2.0.0",
    "express": "^4.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1"
  }
}
